{
  "std::cout": {
    "prefix": "co",
    // "body": ["std::cout << $1 << std::endl;", "$0"],
    "body": ["std::cout << $1 << ${2:std::endl};", "$0"],
    "description": "Prints to the console"
  },
  "cout equal": {
    "prefix": "ce",
    "body": ["std::cout << \"${2:$1 = }\" << $1 << std::endl;", "$0"],
    "description": "Prints to the console"
  },
  "newline": {
    "prefix": "nl",
    "body": ["std::cout << std::endl;"],
    "description": "Prints newline to the console"
  },
  "namespace": {
    "prefix": "nsp",
    "body": ["namespace $1 {", "$0", "}  // end namespace $1"],
    "description": "Namespace declaration"
  },
  "main": {
    "prefix": "main",
    "body": [
      "#include <iostream>",
      "",
      "auto main(int argc, char *argv[]) -> int {",
      "",
      "  return EXIT_SUCCESS;",
      "}"
    ]
  }
}
